[autoload]

_b="*res://Common/Bucket.gd"
EntityMgr="*res://Entity/EntityManager/EntityManager.gd"

[debug]

shapes/collision/shape_color=Color(1, 1, 1, 0.419608)
settings/fps/force_fps=60

[display]

